{
  "gitbook": "3.2.3",
  "title": "Сознание и существование",
  "author": "Viktor Ageyev",
  "plugins": [
    "-sharing",
    "github",
    "simple-page-toc",
    "anchor-navigation-ex"
  ],
  "variables": {
    "host": "ageyev.github.io"
  },
  "links": {
  },
  "pluginsConfig": {
    "theme-default": {
      "showLevel": false
    },
    "github": {
      "url": "https://github.com/ageyev/ageyev.github.io"
    },
    "simple-page-toc": {
      "maxDepth": 3,
      "skipFirstH1": true
    },
    "anchor-navigation-ex": {
      "showLevel": false,
      "showGoTop": true,
      "isRewritePageTitle": false,
      "tocLevel1Icon": "fa fa-hand-o-right",
      "tocLevel2Icon": "fa fa-hand-o-right",
      "tocLevel3Icon": "fa fa-hand-o-right"
    }
  }
}
